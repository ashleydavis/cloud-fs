{
    "name": "cloud-fs",
    "version": "1.0.0",
    "description": "Commands for working with files in cloud storage.",
    "scripts": {
        "start": "node ./build/cloud.js",
        "sd": "npm run start:dev",
        "start:dev": "nodemon",
        "c": "npm run clean",
        "clean": "rm -rf build/*",
        "b": "npm run build",
        "build": "tsc --incremental",
        "bw": "npm run build:watch",
        "build:watch": "tsc  --incremental --watch",
        "prepublishOnly": "npm test && npm run clean && tsc",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "pretest": "npm run lint",
        "t": "npm run test",
        "test": "jest",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch"
    },
    "bin": {
        "cloud": "./bin/cloud"
    },
    "repository": {
        "type": "git",
        "url": "git+https://ashleydavis@github.com/ashleydavis/cloud-fs.git"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ashleydavis/cloud-fs/issues"
    },
    "homepage": "https://github.com/ashleydavis/cloud-fs#readme",
    "dependencies": {
        "@azure/storage-blob": "^12.4.1",
        "moment": "^2.29.1",
        "shelljs": "^0.8.4",
        "vorpal": "^1.12.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.25",
        "@types/shelljs": "^0.8.8",
        "@types/vorpal": "^1.12.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "source-map-support": "0.5.19",
        "ts-jest": "^26.5.0",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^4.1.3"
    }
}
